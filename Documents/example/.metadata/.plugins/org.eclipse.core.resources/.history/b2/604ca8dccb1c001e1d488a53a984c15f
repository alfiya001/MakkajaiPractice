
public class Node {
	int val;
	Node next;
	Node(){}
	Node(int n){
		val = n;
	}
	
	Node head = null;
	Node tail = null;
	
	public void addNode(int val) {
		Node node = new Node(val);
		if(head==null) {
			head = node;
			tail = node;
		}
		else
		{tail.next = node;
		node.next = null;
		tail = node;
		}
		
	}
	
	public static void main(String[] args) {
		Node n = new Node();
		addNode(5);
		
	}
}
